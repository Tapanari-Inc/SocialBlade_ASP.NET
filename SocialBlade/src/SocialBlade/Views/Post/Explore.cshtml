@model string
@using SocialBlade.ViewComponents

@{
    ViewData["Title"] = "Edit";
}
@section stylesheets {
    <link rel="stylesheet" href="/css/styleExplore.css">
}
@section scripts{
    <script src="/js/initExplore.js"></script>
    <script src="/js/reactionLogic.js"></script>
}

@{await Html.RenderPartialAsync("_Menu");}

@await Component.InvokeAsync(typeof(PostListComponent), new { id = Model, viewName = "Explore" })

@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}