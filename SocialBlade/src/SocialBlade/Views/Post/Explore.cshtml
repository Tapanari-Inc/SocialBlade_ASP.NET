@model string
@using SocialBlade.ViewComponents

@section stylesheets {
    <link rel="stylesheet" href="/css/styleExplore.css">
}
@{
    ViewData["Title"] = "Explore";
}
@section scripts{
    <script src="/js/initExplore.js"></script>
    <script src="/js/followButtonsLogic.js"></script>
    <script src="/js/reactionLogic.js"></script>
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

@{await Html.RenderPartialAsync("_Menu");}
@await Component.InvokeAsync(typeof(PostListComponent), new { type = PostListComponent.ListType.Explore, userId = Model })